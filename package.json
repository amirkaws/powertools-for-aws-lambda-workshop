  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/aws-samples/aws-lambda-powertools-typescript-workshop/issues"
  },
  "homepage": "https://github.com/aws-samples/aws-lambda-powertools-typescript-workshop#readme",
  "devDependencies": {
    "ts-node": "^10.9.1"
  }
}